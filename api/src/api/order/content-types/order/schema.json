{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "order"
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "order_histories"
    },
    "orderStatus": {
      "type": "enumeration",
      "enum": [
        "Pending",
        "Completed",
        "Shipped",
        "Canceled"
      ]
    },
    "total_price": {
      "type": "decimal"
    },
    "address": {
      "type": "string",
      "required": true
    }
  }
}
